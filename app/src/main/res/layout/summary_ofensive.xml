        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:id="@+id/CountOfBrejks"
            android:textSize="@dimen/font_size"
            android:layout_below="@+id/PredictionOf7mShots"
            android:layout_alignEnd="@+id/PredictionOf7mShots"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:id="@+id/PredictionOfBrejkShots"
            android:textSize="@dimen/font_size"
            android:layout_below="@+id/CountOfBrejks"
            android:layout_alignEnd="@+id/CountOfBrejks"/>
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/relativeLayout7"
        android:layout_centerHorizontal="true">


        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/Table">
        </LinearLayout>
    </ScrollView>


</RelativeLayout>