<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true"
    android:layout_alignParentTop="true"
    android:layout_alignParentBottom="true"
    tools:context=".SummaryPackage.Summary"
    android:keepScreenOn="true">

    <RelativeLayout
        android:id="@+id/OfensiveStatistic"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="16dp"
        android:visibility="visible"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="false"
        android:layout_alignParentEnd="false">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pocet utokov:"
            android:id="@+id/textView8"
            android:textSize="@dimen/font_size"
            android:visibility="visible"
            android:layout_below="@+id/textView10"
            android:layout_alignStart="@+id/textView11"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cas v utoku:"
            android:id="@+id/textView10"
            android:textSize="@dimen/font_size"
            android:visibility="visible"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/textView8"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Vyuzite utoky:"
            android:id="@+id/textView11"
            android:textSize="@dimen/font_size"
            android:visibility="visible"
            android:layout_below="@+id/textView8"
            android:layout_toStartOf="@+id/textView112"/>

        <TextView
            android:layout_width="1dp"
            android:layout_height="1dp"
            android:id="@+id/textView112"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="00:00"
            android:id="@+id/TimeInOfensive"
            android:textSize="@dimen/font_size"
            android:layout_above="@+id/textView8"
            android:layout_toEndOf="@+id/textView112"
            android:layout_marginStart="42dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:id="@+id/CountOfOfensive"
            android:textSize="@dimen/font_size"
            android:layout_below="@+id/TimeInOfensive"
            android:layout_alignEnd="@+id/TimeInOfensive"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:id="@+id/WinOfensiveCount"
            android:textSize="@dimen/font_size"
            android:layout_below="@+id/CountOfOfensive"
            android:layout_alignEnd="@+id/CountOfOfensive"/>

    </RelativeLayout>

    <TextView
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:id="@+id/textView113"
        android:layout_below="@+id/OfensiveStatistic"
        android:layout_centerHorizontal="true"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/OfensiveStatistic"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/textView113"
        android:id="@+id/relativeLayout7"
        android:gravity="center_horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pocet striel:"
            android:id="@+id/textView9"
            android:textSize="@dimen/font_size"
            android:visibility="visible"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Uspesnost strelby:"
            android:id="@+id/textView12"
            android:layout_below="@+id/textView9"
            android:layout_alignLeft="@+id/textView9"
            android:layout_alignStart="@+id/textView9"
            android:textSize="@dimen/font_size"
            android:visibility="visible"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pocet striel 6m:"
            android:id="@+id/textView104"
            android:textSize="@dimen/font_size"
            android:visibility="visible"
            android:layout_below="@+id/textView12"
            android:layout_alignParentStart="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Uspesnost strelby 6m:"
            android:id="@+id/textView105"
            android:textSize="@dimen/font_size"
            android:layout_below="@+id/textView104"
            android:layout_alignParentStart="true"
            android:visibility="visible"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pocet striel 9m:"
            android:id="@+id/textView108"
            android:textSize="@dimen/font_size"
            android:visibility="visible"
            android:layout_below="@+id/textView105"
            android:layout_alignParentStart="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Uspesnost strelby 9m:"
            android:id="@+id/textView109"
            android:textSize="@dimen/font_size"
            android:layout_below="@+id/textView108"
            android:layout_alignParentStart="true"
            android:visibility="visible"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:id="@+id/CountOfShots"
            android:textSize="@dimen/font_size"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/textView105"
            android:layout_marginStart="42dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:id="@+id/PredictionOfShots"
            android:textSize="@dimen/font_size"
            android:layout_below="@+id/CountOfShots"
            android:layout_alignEnd="@+id/CountOfShots"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:id="@+id/CountOf6mShots"
            android:textSize="@dimen/font_size"
            android:layout_below="@+id/PredictionOfShots"
            android:layout_alignEnd="@+id/PredictionOfShots"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:id="@+id/PredictionOf6mShots"
            android:textSize="@dimen/font_size"
            android:layout_below="@+id/CountOf6mShots"
            android:layout_alignEnd="@+id/CountOf6mShots"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:id="@+id/CountOf9mShots"
            android:textSize="@dimen/font_size"
            android:layout_below="@+id/PredictionOf6mShots"
            android:layout_alignEnd="@+id/PredictionOf6mShots"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:id="@+id/PredictionOf9Shots"
            android:textSize="@dimen/font_size"
            android:layout_below="@+id/CountOf9mShots"
            android:layout_alignEnd="@+id/CountOf9mShots"/>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/OfensiveStatistic"
        android:layout_toEndOf="@+id/textView113"
        android:gravity="center_horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pocet striel kridlo:"
            android:id="@+id/textView59"
