            android:visibility="visible"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pocet striel 7m:"
            android:id="@+id/textView106"
            android:textSize="@dimen/font_size"
            android:visibility="visible"
            android:layout_below="@+id/textView60"
            android:layout_alignParentStart="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Uspesnost strelby 7m:"
            android:id="@+id/textView107"
            android:textSize="@dimen/font_size"
            android:layout_below="@+id/textView106"
            android:layout_alignParentStart="true"
            android:visibility="visible"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pocet brejkov:"
            android:id="@+id/textView110"
            android:textSize="@dimen/font_size"
            android:visibility="visible"
            android:layout_below="@+id/textView107"
            android:layout_alignParentStart="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Uspesnost brejkov:"
            android:id="@+id/textView111"
            android:textSize="@dimen/font_size"
            android:visibility="visible"
            android:layout_below="@+id/textView110"
            android:layout_alignParentStart="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:id="@+id/CountOfWingShots"
            android:textSize="@dimen/font_size"
            android:layout_marginStart="42dp"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/textView60"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:id="@+id/PredictionOfWingShots"
            android:textSize="@dimen/font_size"
            android:layout_below="@+id/CountOfWingShots"
            android:layout_alignEnd="@+id/CountOfWingShots"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:id="@+id/CountOf7mShots"
